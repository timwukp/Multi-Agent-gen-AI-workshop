# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# Bedrock Configuration
BEDROCK_MODEL_ID=us.amazon.nova-pro-v1:0
BEDROCK_REGION=us-west-2

# AgentCore Configuration (will be set after deployment)
AGENTCORE_MEMORY_ROLE_ARN=
AGENTCORE_CONTROL_ENDPOINT=
AGENTCORE_DATA_ENDPOINT=

# Knowledge Base Configuration (will be set after creation)
BEDROCK_KNOWLEDGE_BASE_ID=
BEDROCK_KNOWLEDGE_BASE_DATA_SOURCE_ID=

# Observability Configuration
OBSERVABILITY_ENVIRONMENT=development
OBSERVABILITY_LOG_LEVEL=INFO
OBSERVABILITY_DEBUG_MODE=false

# Tracing Configuration
OBSERVABILITY_TRACING_ENABLED=true
OBSERVABILITY_TRACING_SERVICE_NAME=bedrock-workshop-agents
OBSERVABILITY_TRACING_SERVICE_VERSION=1.0.0
OBSERVABILITY_TRACING_SAMPLE_RATE=1.0

# Metrics Configuration
OBSERVABILITY_METRICS_ENABLED=true
OBSERVABILITY_METRICS_NAMESPACE=BedrockWorkshop/Agents
OBSERVABILITY_METRICS_FLUSH_INTERVAL_SECONDS=60

# Health Monitoring Configuration
OBSERVABILITY_HEALTH_ENABLED=true
OBSERVABILITY_HEALTH_CHECK_INTERVAL_SECONDS=30
OBSERVABILITY_HEALTH_TIMEOUT_SECONDS=10